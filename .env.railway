# DataForSEO Credentials (REQUIRED)
DATAFORSEO_USERNAME=your_dataforseo_username
DATAFORSEO_PASSWORD=your_dataforseo_password

# Port (Railway will set this automatically)
PORT=3000

# Optional: Enable specific modules (leave empty for all modules)
# Example: ENABLED_MODULES=serp,keywords_data,dataforseo_labs
ENABLED_MODULES=

# Optional: Return full API responses (default: false)
DATAFORSEO_FULL_RESPONSE=false

# Optional: Field configuration file path
# FIELD_CONFIG_PATH=./field-config.json